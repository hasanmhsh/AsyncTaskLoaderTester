<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="horizontal" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hms.sra.asynctaskloadertester.MainActivity">


    <EditText
    android:id="@+id/taskETID"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:ems="5"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="enqueue"
        android:onClick="onEnqueueTaskClickeListener"/>

    <EditText
        android:id="@+id/resultETID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="5"/>


</LinearLayout>
